{
    "transitions": {},
    "transitionDuration": {
        "DEFAULT": "300ms"
    },
    "animations": {
        "shake": "shake 200ms 2 ease-in-out"
    },
    "keyframes": {
        "shake": {
            "0%, 100%": {
                "transform": "translateX(0px)"
            },
            "25%": {
                "transform": "translateX(3px)"
            },
            "75%": {
                "transform": "translateX(-3px)"
            }
        }
    }
}